<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:key="plugin_template"
    android:title="@string/app_name">

    <CheckBoxPreference
        android:key="status_plugin_template"
        android:defaultValue="false"
        android:persistent="true"
        android:title="Start/stop plugin" />

    <EditTextPreference
        android:key="bitalino_mac_address"
        android:persistent="true"
        android:title="MAC"
        />

    <MultiSelectListPreference
        android:defaultValue="@array/default_bitalino_channels"
        android:key="bitalino_channels"
        android:persistent="true"
        android:title="Channels"
        android:entries="@array/bitalino_channels_entries"
        android:entryValues="@array/bitalino_channels_entry_values"
        />



</PreferenceScreen>